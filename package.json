{
  "name": "natours",
  "version": "1.0.0",
  "description": "Natours landing page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compiled:sass": "node-sass sass/main.scss css/style.comp.css ",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css  -o css/style.prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MennaRamadan/Natours.git"
  },
  "author": "Menna Ramadan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MennaRamadan/Natours/issues"
  },
  "homepage": "https://github.com/MennaRamadan/Natours#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.1"
  },
  "dependencies": {
    "concat": "^1.0.3"
  }
}
